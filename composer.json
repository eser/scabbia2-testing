{
    "name": "eserozvataf/scabbia2-testing",
    "description": "Scabbia2 Testing Component",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "eserozvataf",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "testing"
    ],
    "homepage": "https://github.com/eserozvataf/scabbia2",
    "license": [
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Eser Ozvataf",
            "homepage": "http://eser.ozvataf.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/eserozvataf/scabbia2-testing/blob/master/contributors.md"
        }
    ],
    "support": {
        "issues": "https://github.com/eserozvataf/scabbia2-testing/issues",
        "source": "https://github.com/eserozvataf/scabbia2-testing"
    },
    "require": {
        "php": ">=5.6.0",
        "eserozvataf/scabbia2-formatters": "*",
        "eserozvataf/scabbia2-helpers": "*"
    },
    "require-dev": {
        "eserozvataf/scabbia2-tasks": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
        "ext-xdebug": "Required for coverage reports"
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\Testing\\": [ "src/Testing/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\Testing\\": [ "tests/Testing/" ]
        }
    }
}
